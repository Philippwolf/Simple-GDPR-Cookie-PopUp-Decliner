{
    "$schema": "https://raw.githubusercontent.com/Philippwolf/Simple-GDPR-Cookie-PopUp-Decliner/master/rules.schema.json",
    "laitapro": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": "#tarteaucitronAlertBig #tarteaucitronPersonalize"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": "#tarteaucitronPersonalize",
                            "displayFilter": true
                        }
                    }
                ]
            }
        ],
        "methods": [
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "hide",
                            "target": {
                                "selector": ".tarteaucitronBeforeVisible"
                            }
                        }
                    ]
                },
                "name": "HIDE_CMP"
            },
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": "#tarteaucitronCloseAlert"
                    }
                },
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "consent",
                            "consents": [
                                {
                                    "trueAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#analyticsAllowed"
                                        }
                                    },
                                    "falseAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#analyticsDenied"
                                        }
                                    },
                                    "type": "B"
                                }
                            ]
                        },
                        {
                            "type": "consent",
                            "consents": [
                                {
                                    "trueAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#addthisAllowed"
                                        }
                                    },
                                    "falseAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#addthisDenied"
                                        }
                                    },
                                    "type": "F"
                                }
                            ]
                        },
                        {
                            "type": "consent",
                            "consents": [
                                {
                                    "trueAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#youtubeAllowed"
                                        }
                                    },
                                    "falseAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "#youtubeDenied"
                                        }
                                    },
                                    "type": "A"
                                }
                            ]
                        }
                    ]
                },
                "name": "DO_CONSENT"
            },
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": "#tarteaucitronClosePanel"
                    }
                },
                "name": "SAVE_CONSENT"
            },
            {
                "name": "UTILITY"
            }
        ]
    }
}
